# Environment variables for BbtToken deployment
# Copy this file to .env and fill in your values

# Network Configuration
INFURA_ID=your_infura_project_id_here
ACCOUNT=your_private_key_here

# Network URLs (uncomment and configure as needed)
# SEPOLIA=https://sepolia.infura.io/v3/your_infura_id
# TESTNET=https://data-seed-prebsc-1-s1.binance.org:8545/
# MAINNET=https://bsc-dataseed.binance.org/

# API Keys for verification
ETHERSCAN=your_etherscan_api_key_here

# Token Configuration
TOKEN_NAME=BabyBoomToken
TOKEN_SYMBOL=BBT
TOTAL_SUPPLY=1000000000
INITIAL_OWNER=0x... # Leave empty to use deployer address

# Legacy configuration (for existing deploy.js)
DECIMAL=18
MINT_TARGET=0x...
INITIAL_MINT=1000000000000000000000000000
